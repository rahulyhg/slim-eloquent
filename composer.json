{
    "name": "slimphp-api/slim-eloquent",
    "description": "A module for slim-api for eloquent integration, using eloquent as the ORM",
    "keywords": ["slim","api","eloquent","orm","slimapi"],
    "require": {
        "php": ">5.6",
        "illuminate/database": "^5.1",
        "illuminate/events": "^5.1",
        "slimphp-api/slim-api": "*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.1",
        "mikey179/vfsStream": "^1.6",
        "satooshi/php-coveralls": "^0.7@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Baker",
            "email": "me@g403.co"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {"SlimApi\\Eloquent\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"SlimApi\\EloquentTest\\": "tests/phpunit/"}
    }
}
